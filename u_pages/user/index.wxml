        </block>

            <view class="hot">
                <view class="serve-project">
                    <view class="image" style="display: flex; align-items: center; justify-content: center;">
                        <image style="width: 30rpx;height: 30rpx;" src="../../assets/images/user/hot_icon.png"></image>
                    </view>
                    <text class="text">热销服务项目</text>
                </view>
                <view class="white-box project-list" wx:for="{{list}}" wx:key='index' style="margin-top: 24rpx;">
                    <view class="item-top">
                        <view class="item-name">
                            {{item.name}}
                        </view>
                        <view class="more" bindtap="golaw" data-id="{{item.id}}" data-num="{{item.finance_num}}" data-fid="{{item.financeid}}">了解产品</view>
                    </view>
                    <view class="item-content">
                        <block wx:if="{{item.catid == 3}}">
                        <view class="content-item">
                            <view class="sp">成交金额（万元）</view>
                            <view class="p">{{item.cj_price}}</view>
                        </view>
                        </block>
                        <block wx:else>
                        <view class="content-item">
                            <view class="sp">服务价格（元）</view>
                            <view class="p">{{item.cj_price}}</view>
                        </view>
                        </block>
                        <view class="content-item">
                            <view class="sp">办理笔数</view>
                            <view class="p">{{item.bl_num}}</view>
                        </view>
                        <view class="content-item">
                            <view class="sp">好评</view>
                            <view class="p">{{item.hp_num}}</view>
                        </view>
                    </view>
                    <block wx:if="{{item.finance_num == 1}}">
                    <view class="item-bottom" bindtap="getFinance" data-id="{{item.financeid}}" data-lawid="{{item.id}}">
                        <view class="name">
                            <text class="t">{{item.jg_name}}</text>
                            <image class="img" src="../../assets/images/text.png"></image>
                        </view>
                        <view class="more">
                            <image class="img" src="../../assets/images/more.png"></image>
                        </view>
                    </view>
                </block>
                </view>

            </view>
   
    </view>
</view>
