.main {
    height: 100vh;
    background-color: #f5f5f5;
}

.nav {
    background: #ffffff;
    box-sizing: border-box;
    border-top: 2rpx solid #eeeeee;
}

.list-item {
    height: calc(100vh - 110rpx);
    background: #F5F5F5;
    border-top: 2rpx solid #e5e5e5;
    position: relative;
}

.van-cell__title {
    font-weight: bold !important;
    font-size: 28rpx;
}

.item {
    padding-top: 30rpx;
    position: relative;
}

.item:after {
    content: '';
    width: 6rpx;
    height: 32rpx;
    background: #0B7BF9;
    opacity: 1;
    border-radius: 4rpx;
    position: absolute;
    left: 0;
    top: 55rpx;
}

.text-item {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx 0;
    border-bottom: 1px solid #eeeeee;
}

.text-item text {
    font-size: 28rpx;
    color: #666666;
}
.project{
    display: flex;
    flex-direction: column;
    padding: 20rpx 0;
}
.project>text:nth-child(1){
    font-size: 28rpx;
    color: #666666;
}
.project .flex{
    display: flex;
    flex-flow: row wrap;
    justify-content: flex-start;
    align-items: center;
    padding-top: 30rpx;
}
.project .flex text{
    height: 56rpx;
    background: #FFFFFF;
    border: 1px solid #CCCCCC;
    opacity: 1;
    border-radius: 32rpx;
    line-height: 56rpx;
    display: inline-block;
    padding: 0 32rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}