.tabbar{
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 132rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-top: 1px solid #eeeeee;
    background-color: #fff;
    z-index: 100;
}
.tabbar-item {
    height: 132rpx;
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.tabbar-item .t{
    font-size: 24rpx;
    color: #333333;
    padding-top: 6rpx;
}
.tabbar-item:nth-child(1) .img{
    width: 52rpx;
    height: 52rpx;
}
.tabbar-item:nth-child(2) .img{
    width: 52rpx;
    height: 52rpx;
}
.tabbar-item:nth-child(3) .img{
    width: 42rpx;
    height: 52rpx;
} 
